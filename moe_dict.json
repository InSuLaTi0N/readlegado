
{
  "enabled": true,
  "name": "萌娘百科",
  "urlRule": "https://zh.moegirl.org.cn/api.php?action=query&format=json&prop=extracts&exintro=&titles={{key}}",
  "showRule": "@js:\n" +
    "var obj = JSON.parse(result);\n" +
    "var pages = obj.query.pages;\n" +
    "var firstId = Object.keys(pages)[0];\n" +
    "pages[firstId].extract || \"无条目或摘要\";"
}